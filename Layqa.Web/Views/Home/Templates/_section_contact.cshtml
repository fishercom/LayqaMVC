@using Layqa.Service
@using Layqa.Service.Util
@using Layqa.Service.Dto
@using Layqa.Web.Models
@model ArticleDto
@{
    ServiceArticle serviceArticle = new ServiceArticle();
    ContactFormViewModel form_model = new ContactFormViewModel();
    var lSection = serviceArticle.GetWebList(Model.ArticleId);
}
<!-- Contact Section -->
<section id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">@Model.Title</h2>
                <h3 class="section-subheading text-muted">@Model.SubTitle</h3>
            </div>
        </div>
        <div class="row">
            <div id="form_contact" class="col-lg-12">

                @Html.Partial("Templates/_form_contact", form_model)

            </div>
        </div>
    </div>
</section>
